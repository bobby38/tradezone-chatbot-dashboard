# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# AI API Keys
NEXT_PUBLIC_OPENAI_API_KEY=sk-proj-your-openai-api-key
NEXT_PUBLIC_OPENROUTER_API_KEY=sk-or-your-openrouter-api-key

# SMTP Configuration for Email Reports
# For Gmail: Use smtp.gmail.com, port 587
# Enable 2-factor authentication and create an App Password
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Alternative SMTP Providers:
# Outlook: smtp-mail.outlook.com, port 587
# Yahoo: smtp.mail.yahoo.com, port 587
# Custom SMTP: your-smtp-server.com, port 587

# WooCommerce REST API Configuration
WC_SITE=https://your-store.com
WC_KEY=ck_your_consumer_key
WC_SECRET=cs_your_consumer_secret

# Google Analytics 4 Configuration
GA_PROPERTY=properties/123456789
GOOGLE_APPLICATION_CREDENTIALS=/path/to/ga-service.json

# Google Search Console Configuration
SC_SITE=https://your-store.com
